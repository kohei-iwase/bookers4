<h1>New book</h1>

<!-- form_for部分 -->
<%= form_for(@book, url: '/books') do |f| %>
<!-- エラー部分 -->
<%= render 'layouts/error_messages', model: f.object %>
    <h4>Title</h4>
    <%= f.text_field :title %>
    <h4>Opinion</h4>
    <%= f.text_area :opinion %>

 <br>
    <%= f.submit 'Create Book' %>
<% end %>